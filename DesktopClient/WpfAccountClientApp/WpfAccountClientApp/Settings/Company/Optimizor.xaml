        <Label x:Name="lblStatus5" Content="Status" HorizontalAlignment="Left" Height="25" Margin="30,0,0,241" VerticalAlignment="Bottom" Width="402" FontFamily="Century Gothic" HorizontalContentAlignment="Right" VerticalContentAlignment="Bottom" FontSize="14"/>
        <Label x:Name="lblStatus6" Content="Status" HorizontalAlignment="Left" Height="25" Margin="30,0,0,216" VerticalAlignment="Bottom" Width="402" FontFamily="Century Gothic" HorizontalContentAlignment="Right" VerticalContentAlignment="Bottom" FontSize="14"/>
        <Label x:Name="lblStatus7" Content="Status" HorizontalAlignment="Left" Height="25" Margin="30,0,0,191" VerticalAlignment="Bottom" Width="402" FontFamily="Century Gothic" HorizontalContentAlignment="Right" VerticalContentAlignment="Bottom" FontSize="14"/>
